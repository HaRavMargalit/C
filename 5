void f1()
{

  
  int j;
  do
  {
   printf("enter positive number\n");
   scanf("%d",&j);
  }while(j<0);
  if( j%2==0)
  {
    printf("enter positive number and the number is defined by two\n");
     printf("%d", j);  
  }
else 
  if (j%2==1)
  {
printf("enter positive number and the number is not defined by two\n");
  }



}
